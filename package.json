{
  "name": "vite-plugin-generate-html-i18n",
  "description": "Vite plugin for generating html i18n files",
  "author": "lFandoriNl",
  "private": false,
  "version": "2.0.0",
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "vite",
    "plugin",
    "generate",
    "html",
    "i18n",
    "internationalization"
  ],
  "license": "MIT",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "module": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch"
  },
  "dependencies": {
    "glob": "^10.3.10",
    "jsdom": "^22.1.0"
  },
  "devDependencies": {
    "@changesets/cli": "^2.26.2",
    "@types/jsdom": "^21.1.4",
    "@types/node": "^20.8.3",
    "tsup": "^7.2.0",
    "typescript": "^5.0.2",
    "vite": "^4.4.5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lFandoriNl/vite-plugin-generate-html-i18n"
  }
}
